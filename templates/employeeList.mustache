<table class="table table-striped">
    <h1>Employee List</h1>
    <thead>
    <tr>
        <th>Name</th>
        <th>Room</th>
        <th>Phone</th>
        <th>Job</th>
    </tr>
    </thead>
    <tbody>
    {{# employee }}
        <tr>
            <td><a href="employeeDetail.php?employee_id={{ employee_id }}">{{surname}} {{ employeeName }}</a></td>
            <td>{{ roomName }}</td>
            <td>{{ phone }}</td>
            <td>{{ job }}</td>
        </tr>
    {{/employee}}
    </tbody>
</table>
